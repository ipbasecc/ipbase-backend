{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "type": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "boards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::board.board",
      "mappedBy": "project"
    },
    "overviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::overview.overview",
      "mappedBy": "project"
    },
    "default_version": {
      "type": "integer"
    },
    "todogroups": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todogroup.todogroup",
      "mappedBy": "project"
    },
    "chat_service": {
      "type": "json"
    },
    "invite_uris": {
      "displayName": "invite_uri",
      "type": "component",
      "repeatable": true,
      "component": "project.invite-uri"
    },
    "jsonContent": {
      "type": "json"
    },
    "private": {
      "type": "boolean",
      "default": true
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "created_projects"
    },
    "blocked": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "storages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::storage.storage",
      "mappedBy": "belonged_project"
    },
    "members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "projects"
    },
    "mm_channel_id": {
      "type": "uid"
    },
    "preferences": {
      "type": "json"
    },
    "schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::schedule.schedule",
      "mappedBy": "by_project"
    },
    "project_members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::member.member",
      "mappedBy": "by_projects"
    },
    "member_roles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::member-role.member-role",
      "mappedBy": "by_project"
    },
    "readonly": {
      "type": "boolean",
      "default": false
    },
    "protected": {
      "type": "boolean",
      "default": false
    },
    "project_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::document.document",
      "mappedBy": "by_project"
    },
    "by_team": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::team.team",
      "inversedBy": "projects"
    },
    "sub_projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "parent_project"
    },
    "parent_project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "sub_projects"
    },
    "mm_channel": {
      "type": "json"
    },
    "budgets": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::budget.budget",
      "mappedBy": "project"
    },
    "meeting": {
      "type": "boolean",
      "default": false
    },
    "private_meets": {
      "type": "json"
    },
    "total_filesize": {
      "type": "decimal"
    }
  }
}
